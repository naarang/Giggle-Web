{
  "name": "giggle-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "find-unused-exports": "ts-prune",
    "knip": "knip",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "vitest run --coverage --coverage.reporter=html",
    "test:report": "vitest run --reporter=html --outputFile=./test-report/index.html",
    "debug:test-parsing": "node debug-test-parsing.cjs",
    "debug:workflow": "act pull_request -W .github/workflows/test-report.yml",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:install": "playwright install"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.15",
    "@types/react-signature-canvas": "^1.0.6",
    "axios": "^1.7.7",
    "embla-carousel-autoplay": "^8.5.2",
    "embla-carousel-react": "^8.5.2",
    "framer-motion": "^11.11.9",
    "react": "^18.3.1",
    "react-daum-postcode": "^3.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.0",
    "react-kakao-maps-sdk": "^1.1.27",
    "react-router-dom": "^6.27.0",
    "react-signature-canvas": "^1.0.6",
    "tailwind-scrollbar-hide": "^1.1.7",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "3.1.4",
    "@vitest/ui": "3.1.4",
    "autoprefixer": "^10.4.20",
    "cloc": "2.4.0-cloc",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "globals": "^15.11.0",
    "jsdom": "^26.1.0",
    "knip": "^5.56.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.14",
    "ts-prune": "^0.10.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "vite": "^5.4.9",
    "vite-bundle-analyzer": "^0.21.0",
